{
  "name": "@fbi-js/factory-factory",
  "version": "1.2.1",
  "description": "factory for fbi factory development",
  "main": "lib/index.js",
  "files": [
    "lib",
    "templates"
  ],
  "scripts": {
    "build": "rm -rf lib && tsc",
    "dev": "npm run build && tsc --watch",
    "lint": "eslint src --fix",
    "format": "prettier --write \"src/**/*.{ts,js,json}\""
  },
  "keywords": [],
  "author": "shaw @neikvon",
  "dependencies": {
    "ejs": "^3.1.5",
    "fbi": "^4.3.2",
    "typescript": "^4.1.3"
  },
  "devDependencies": {
    "@fbi-js/eslint-config-typescript": "^1.1.0",
    "@fbi-js/tsconfig": "^1.1.1",
    "@types/ejs": "^3.0.5",
    "@types/node": "14.14.20",
    "eslint": "^7.17.0",
    "prettier": "^2.2.1"
  },
  "eslintConfig": {
    "extends": [
      "@fbi-js/typescript"
    ]
  },
  "prettier": {
    "semi": false,
    "tabWidth": 2,
    "singleQuote": true,
    "trailingComma": "none",
    "endOfLine": "auto"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fbi-js/factory-factory.git"
  },
  "homepage": "https://github.com/fbi-js/factory-factory",
  "bugs": {
    "url": "https://github.com/fbi-js/factory-factory/issues"
  }
}
